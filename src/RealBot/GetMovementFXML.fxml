<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root prefHeight="326.0" prefWidth="416.0" stylesheets="@RealBotStyleSheet.css" type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox styleClass="backgroundstyle">
         <children>
            <Region prefHeight="31.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
            <Label alignment="CENTER" style="-fx-text-fill: black;" styleClass="big_letters" text="New Movement">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
            <Region prefHeight="54.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
         </children>
         <padding>
            <Insets bottom="4.0" top="4.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <GridPane hgap="10.0" styleClass="foregroundstyle" vgap="5.0" VBox.vgrow="ALWAYS">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="290.0" minWidth="10.0" prefWidth="250.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="203.0" minWidth="10.0" prefWidth="166.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints maxHeight="33.0" minHeight="10.0" prefHeight="28.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="178.0" minHeight="10.0" prefHeight="38.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="141.0" minHeight="1.0" prefHeight="35.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="239.0" minHeight="10.0" prefHeight="151.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="197.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label styleClass="big_letters" text="Description" GridPane.rowIndex="2" />
            <Label styleClass="big_letters" text="Name" />
            <Label styleClass="big_letters" text="Type" GridPane.columnIndex="1" />
            <TextArea fx:id="descriptionTextArea" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="2" GridPane.rowIndex="3" />
            <TextField fx:id="nameTextField" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="movementTypeChooser" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0" GridPane.columnIndex="1" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
                  <Button fx:id="addButton" mnemonicParsing="false" text="Add" />
               </children>
               <padding>
                  <Insets left="28.0" />
               </padding>
            </HBox>
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" />
         </padding>
      </GridPane>
   </children>

</fx:root>
