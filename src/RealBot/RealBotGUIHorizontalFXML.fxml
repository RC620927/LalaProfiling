<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root minHeight="140.0" minWidth="1200.0" prefHeight="140" prefWidth="1200.0" styleClass="foregroundstyle" stylesheets="@RealBotStyleSheet.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane hgap="20.0" vgap="10.0" GridPane.rowIndex="1" HBox.hgrow="ALWAYS" VBox.vgrow="ALWAYS">
         <children>
            <HBox GridPane.columnIndex="2" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="big_letters" text="Max Acceleration:" GridPane.columnIndex="2" GridPane.rowIndex="1">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="40.0" prefWidth="109.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="40.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="maxAccelerationLabel" styleClass="big_letters" text="150">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox GridPane.columnIndex="2" GridPane.rowIndex="2">
               <children>
                  <Label styleClass="big_letters" text="Total Time:" GridPane.columnIndex="2" GridPane.rowIndex="2">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="40.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="totalTimeLabel" styleClass="big_letters" text="12.56">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <children>
                  <Label styleClass="big_letters" text="Realistic">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox spacing="5.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="200.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <RadioButton mnemonicParsing="false" styleClass="small_letters" text="Yes" />
                        <RadioButton mnemonicParsing="false" styleClass="small_letters" text="No" />
                     </children>
                     <padding>
                        <Insets top="6.0" />
                     </padding>
                  </HBox>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1">
               <children>
                  <Label styleClass="big_letters" text="Randomness:">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Region prefHeight="200.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <Slider fx:id="randomnessSlider" majorTickUnit="30.0" min="10.0" showTickLabels="true" styleClass="small_letters">
                           <padding>
                              <Insets top="8.0" />
                           </padding>
                        </Slider>
                     </children>
                  </HBox>
               </children></HBox>
            <HBox GridPane.columnIndex="3" GridPane.rowSpan="3">
               <children>
                  <VBox GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0" GridPane.columnIndex="1" GridPane.rowIndex="1" VBox.vgrow="ALWAYS">
                           <VBox.margin>
                              <Insets left="10.0" top="30.0" />
                           </VBox.margin>
                        </HBox>
                        <HBox prefHeight="100.0" prefWidth="200.0" spacing="15.0">
                           <children>
                              <Button fx:id="runButton" mnemonicParsing="false" prefHeight="37.0" prefWidth="134.0" styleClass="big_letters" text="Run">
                                 <font>
                                    <Font size="17.0" />
                                 </font></Button>
                           </children>
                           <VBox.margin>
                              <Insets left="15.0" />
                           </VBox.margin>
                        </HBox>
                     </children>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <VBox prefHeight="200.0" prefWidth="100.0" style="-fx-background-color: #cdcdcd;" GridPane.rowSpan="3">
               <GridPane.margin>
                  <Insets bottom="4.0" left="4.0" />
               </GridPane.margin></VBox>
            <HBox prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <children>
                  <Label styleClass="big_letters" text="Difficulty">
                     <font>
                        <Font size="21.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="100.0" prefWidth="200.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="40.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="difficultyLabel" styleClass="big_letters" text="1.0">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </HBox>
            <HBox GridPane.columnIndex="2">
               <children>
                  <Label styleClass="big_letters" text="Top Speed:" GridPane.columnIndex="2">
                     <font>
                        <Font size="20.0" />
                     </font>
                  </Label>
                  <HBox prefHeight="40.0" prefWidth="166.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Region prefHeight="40.0" prefWidth="0.0" HBox.hgrow="ALWAYS" />
                        <Label fx:id="topSpeedLabel" alignment="CENTER_RIGHT" contentDisplay="RIGHT" styleClass="big_letters" text="150" textAlignment="RIGHT">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
               </children>
            </HBox>
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="NEVER" minWidth="10.0" prefWidth="250.0" />
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="NEVER" maxWidth="170.0" minWidth="170.0" prefWidth="170.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <VBox.margin>
            <Insets left="10.0" />
         </VBox.margin>
         <HBox.margin>
            <Insets top="5.0" />
         </HBox.margin>
      </GridPane>
   </children>
</fx:root>
