<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<fx:root type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
   
         <GridPane prefHeight="100.0" prefWidth="200.0">
           <columnConstraints>
             <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
             <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
               <ChoiceBox fx:id="movementChooser" prefWidth="150.0" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
               <HBox prefHeight="100.0" prefWidth="200.0" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
                  <children>
                     <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                     <Button fx:id="cancelButton" minWidth="60.0" mnemonicParsing="false" prefWidth="60.0" text="Cancel" />
                     <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                     <Button fx:id="addButton" minWidth="60.0" mnemonicParsing="false" prefWidth="60.0" text="Add" />
                     <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
                  </children>
               </HBox>
            </children>
         </GridPane>
   </children>

</fx:root>
